            text_color: 1, 1, 1, 1
            font_size: "35sp"
            font_name: "fonts/Poppins-SemiBold.ttf"
        
        MDLabel:
            text: "Personal Accounting System"
            pos_hint: {"center_x": .5, "center_y": .15}
            halign: "center"
            theme_text_color: "Custom"
            text_color: 1, 1, 1, 1
            font_size: "15sp"
            font_name: "fonts/Poppins-Regular.ttf"