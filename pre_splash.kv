            source: "img/L.png"
            size: self.texture_size
            size_hint: .26,.26
            pos_hint: {"center_x": .5, "center_y": .55}
        
        MDLabel:
            text: "PAS"
            pos_hint: {"center_x": .5, "center_y": .2}
