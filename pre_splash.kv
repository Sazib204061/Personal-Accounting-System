            pos_hint: {"center_x": .5, "center_y": .85}
            halign: "center"
            theme_text_color: "Custom"
            text_color: 1, 1, 1, 1
            font_size: "35sp"
            font_name: "fonts/Poppins-SemiBold.ttf"
